CAAA,GAAA,CAAA,eAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,QAAA,KAAA,CAAA,UAAA,GAAA,GAAA,CAAA,IAAA;AACA,QAAA,KAAA,CAAA,WAAA,GAAA,GAAA,CAAA,IAAA,CAAA,CAAA,EAAA,EAAA;;AACA,QAAA,KAAA,CAAA,SAAA,CAAA,GAAA,CAAA,IAAA,CAAA,CAAA,EAAA,EAAA;AACA,OAJA;AAKA,KAdA;AAeA,IAAA,SAfA,qBAeA,UAfA,EAeA;AAAA;;AACA,MAAA,GAAA,CAAA,GAAA,CAAA,2BAAA,UAAA,EAAA;AACA,QAAA,MAAA,EAAA;AACA,UAAA,OAAA,EAAA,KAAA,OADA;AAEA,UAAA,QAAA,EAAA,KAAA;AAFA;AADA,OAAA,EAKA,IALA,CAKA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,SAAA,GAAA,GAAA,CAAA,IAAA,CAAA,OAAA;AACA,QAAA,MAAA,CAAA,KAAA,GAAA,GAAA,CAAA,IAAA,CAAA,KAAA;;AAEA,QAAA,MAAA,CAAA,SAAA,CAAA,OAAA,CAAA,UAAA,IAAA,EAAA;AACA;AACA,cAAA,CAAA,IAAA,CAAA,IAAA,EAAA;AACA,YAAA,IAAA,CAAA,IAAA,GAAA,CAAA,EAAA,CAAA;AACA,WAFA,MAEA;AACA,YAAA,IAAA,CAAA,IAAA,GAAA,IAAA,CAAA,KAAA,CAAA,IAAA,CAAA,IAAA,CAAA;AACA;AACA,SAPA;AAQA,OAjBA;AAkBA,KAlCA;AAmCA,IAAA,gBAnCA,4BAmCA,QAnCA,EAmCA;AACA,WAAA,QAAA,GAAA,QAAA;AACA,WAAA,SAAA,CAAA,KAAA,UAAA,CAAA,CAAA,EAAA,EAAA;AACA,KAtCA;AAuCA,IAAA,mBAvCA,+BAuCA,OAvCA,EAuCA;AACA,WAAA,OAAA,GAAA,OAAA;AACA,WAAA,SAAA,CAAA,KAAA,UAAA,CAAA,CAAA,EAAA,EAAA;AACA;AA1CA;AAfA,CAAA","sourcesContent":["<template>\r\n  <!--    分类 -->\r\n  <div style=\"margin-top: 10px\">\r\n    <el-card>\r\n      <el-row :gutter=\"10\">\r\n        <!--          分类-->\r\n        <el-col :span=\"4\">\r\n          <div style=\"border: 1px dashed #ccc; text-align: center; border-bottom: none\">\r\n            <div class=\"category-item\" @click=\"getGoodsByCategory(item.id)\" v-for=\"item in categories\" :key=\"item.id\"\r\n                 :class=\"{ active: (activeIndex === item.id)}\">{{ item.name }}\r\n            </div>\r\n          </div>\r\n        </el-col>\r\n\r\n        <el-col :span=\"20\">\r\n          <!--          分类商品-->\r\n          <div style=\"min-height: 150px\">\r\n            <el-row :gutter=\"10\">\r\n              <el-col :span=\"6\" v-for=\"item in tableData\" :key=\"item.id\">\r\n                <div style=\"margin-bottom: 10px; cursor: pointer\" @click=\"goodsDetail(item.id)\">\r\n                  <div style=\"padding: 5px\">\r\n                    <el-image :src=\"item.imgs[0]\" style=\"width: 100%; height: 100px\" fit=\"contain\"></el-image>\r\n                  </div>\r\n                  <div style=\"padding: 5px; height: 20px; text-align: center; font-size: 12px; overflow: hidden\">\r\n                    <el-tooltip :content=\"item.name\" placement=\"bottom\" effect=\"light\">\r\n                      <span class=\"item-title\">{{ item.name }}</span>\r\n                    </el-tooltip>\r\n                  </div>\r\n                  <div style=\"padding: 5px; text-align: center; color: red; font-size: 12px\">\r\n                    ￥ {{ item.price }}\r\n                  </div>\r\n                </div>\r\n              </el-col>\r\n            </el-row>\r\n          </div>\r\n\r\n          <!--      分页-->\r\n          <div style=\"margin-top: 20px\">\r\n            <el-pagination\r\n                small\r\n                @size-change=\"handleSizeChange\"\r\n                @current-change=\"handleCurrentChange\"\r\n                :current-page=\"pageNum\"\r\n                :page-size=\"pageSize\"\r\n                :page-sizes=\"[4, 8, 12]\"\r\n                layout=\" prev, pager, next\"\r\n                :total=\"total\"\r\n            >\r\n            </el-pagination>\r\n          </div>\r\n\r\n        </el-col>\r\n      </el-row>\r\n    </el-card>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport API from \"../../utils/request\";\r\n\r\nexport default {\r\n  name: \"classify\",\r\n  data() {\r\n    return {\r\n      tableData: [],\r\n      activeIndex: 0,\r\n      categories: [],\r\n      pageNum: 1,\r\n      pageSize: 8,\r\n      total: 0\r\n    };\r\n  },\r\n  created() {\r\n    this.load()\r\n  },\r\n  methods: {\r\n    goodsDetail(id) {\r\n      this.$router.replace({path: '/front/goods', query: {id: id}})\r\n    },\r\n    getGoodsByCategory(id) {\r\n      this.activeIndex = id\r\n      this.loadTable(id)\r\n    },\r\n    load() {\r\n      API.get(\"/api/category\").then(res => {\r\n        this.categories = res.data\r\n        this.activeIndex = res.data[0].id\r\n        this.loadTable(res.data[0].id)\r\n      })\r\n    },\r\n    loadTable(categoryId) {\r\n      API.get(\"/api/goods/byCategory/\" + categoryId, {\r\n        params: {\r\n          pageNum: this.pageNum,\r\n          pageSize: this.pageSize\r\n        }\r\n      }).then(res => {\r\n        this.tableData = res.data.records\r\n        this.total = res.data.total\r\n\r\n        this.tableData.forEach(item => {\r\n          // 处理下表格的图片显示\r\n          if (!item.imgs) {\r\n            item.imgs = ['']\r\n          } else {\r\n            item.imgs = JSON.parse(item.imgs)\r\n          }\r\n        })\r\n      })\r\n    },\r\n    handleSizeChange(pageSize) {\r\n      this.pageSize = pageSize;\r\n      this.loadTable(this.categories[0].id)\r\n    },\r\n    handleCurrentChange(pageNum) {\r\n      this.pageNum = pageNum;\r\n      this.loadTable(this.categories[0].id)\r\n    }\r\n  },\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.item-title {\r\n  cursor: pointer;\r\n}\r\n\r\n.item-title:hover {\r\n  color: orangered;\r\n}\r\n.active {\r\n  color: red !important;\r\n}\r\n\r\n.category-item {\r\n  padding: 5px 0;\r\n  border-bottom: 1px dashed #ccc;\r\n  color: #666;\r\n  cursor: pointer;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/front"}]}